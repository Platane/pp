{
  "name": "pitchperfect-ssr",
  "version": "0.1.0",
  "private": true,
  "engines": { "node": "9.9" },
  "dependencies": {
    "emotion": "9.1.2",
    "emotion-server": "9.1.2",
    "koa": "2.5.0",
    "koa-bodyparser": "4.2.0",
    "koa-router": "7.4.0",
    "koa2-cors": "2.0.5",
    "koa2-cors-error": "1.0.0",
    "module-alias": "2.0.6",
    "node-fetch": "2.1.2",
    "preact": "8.2.9",
    "preact-compat": "3.18.0",
    "preact-redux": "2.0.3",
    "preact-render-to-string": "3.7.0",
    "redux": "4.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-emotion": "9.1.2",
    "babel-plugin-flow-runtime": "0.17.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-assets": "1.0.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-inline-environment-variables": "0.4.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.26.0",
    "flow-bin": "0.69.0",
    "flow-runtime": "0.17.0",
    "nodemon": "1.17.3",
    "preact-emotion": "9.1.2",
    "prettier": "1.11.1",
    "tap-spec": "4.1.1",
    "tape": "4.9.0"
  },
  "scripts": {
    "start": "node -e \"require('./lib').create()\"",
    "start:dev": "nodemon start.js",
    "build:test": "rm -rf lib && NODE_ENV=test babel --out-dir ./lib ./src",
    "resetdb":
      "node -e \"require('babel-register');require('./src/__tests__/bootstrap').run()\"",
    "build:staticPage": "node ./script/buildStaticPage.js",
    "build":
      "rm -rf lib && NODE_ENV=production babel --out-dir ./lib --ignore __tests__ ./src",
    "deploy:alias":
      "now alias `now ls pitchperfect-api | grep -m 1 -e pitchperfect-api- | cut -f 2 -d ' ' ` pitchperfect-api",
    "deploy":
      "now -e ENV=production -e GC_JSON_KEY_FILE=@pitchperfect_gc_json_key_file_secret -e PORT=80 --public && npm run deploy:alias"
  }
}
